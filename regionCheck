async function checkRegion(users) {
    switch (users[user.id][0]) {
        case '6': 
            region = "NA"
            break;
        case '7': 
            region = "EU"
            break;
        case '8': 
            region = "AS"
            break;
        case '9': 
            region = "HK/TW"
            break;
        default:
            region = "cringe doodoohead"
            break;
    }
    return region;
}

module.exports = checkRegion